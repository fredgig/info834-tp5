		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.4.4",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"postcss": "^8.4.12",
		"prettier": "^2.6.0",
		"prettier-plugin-svelte": "^2.6.0",
		"prettier-plugin-tailwindcss": "^0.1.8",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.6",
		"svelte-preprocess": "^4.10.4",
